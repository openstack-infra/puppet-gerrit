<% @its_rules.each do |its_rule| -%>
[rule "<%= its_rule['name'] %>"]
  action = <%= its_rule['action'] %>
  <% unless its_rule['event_type'].nil? -%>
    event-type = <%= its_rule['event_type'] %>
  <% end -%>
  <% unless its_rule['label'].nil? -%>
    <% its_rule['label'].each do |its_label| -%>
      <%= its_label['name'] %> = <%= its_label['approvals'] %>
    <% end -%>
  <% end -%>
<% end -%>
